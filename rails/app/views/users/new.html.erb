<% if flash[:danger] %>
    <p style="color:red">
        <%= flash[:danger] %>
    </p>
<% end %>
<%= form_for :user, url: users_path do |f| %>
    <p>
        <%= f.label :username %>
        <%= f.text_field :username %>
    </p>
    <p>
        <%= f.submit %>
    </p>
<% end %>
<%= link_to 'Back', users_path %>